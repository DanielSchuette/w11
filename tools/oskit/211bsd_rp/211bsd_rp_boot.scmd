;
; Setup file for 211bsd RP06 based system
;
; Usage:
;
; pdp11 211bsd_rp_boot.scmd
;
do ../../simh/setup_w11a_max.scmd
set tto 7b
set dlo0 7b
;
set rp0 rp06
set rp1 rp06
;
set rl0 rl02
set rl1 rl02
set rl2 rl02
set rl3 rl02
;
att rp0 211bsd_rp.dsk
att rk6 rk_home.dsk
;
boo rp0
